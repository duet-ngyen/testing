<%= form_for [:teacher, @examination] do |f| %>
  <%#= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.label :name, 'Name' %><br />
    <%= f.text_area :name, rows: 3 %><br />
  </div>
  <%= collection_check_boxes(:examination, :question_ids, @questions, :id, :content) do |s|%>
    <%= s.label("data-value": s.value) {s.check_box + s.text} %></br>
  <% end %>
  <p><%= f.submit 'Submit', class: "btn btn-primary" %></p>
<% end %>
